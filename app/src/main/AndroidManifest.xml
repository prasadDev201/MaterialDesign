<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="main.materialdesign.spm.materialdesign" >


    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <!-- GCM requires a Google account. -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <!-- Creates a custom permission so only this app can receive its messages. -->
    <permission
        android:name="main.materialdesign.spm.materialdesign.permission.C2D_MESSAGE"
        android:protectionLevel="signature" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/MyMaterialTheme" >
        <activity
            android:name=".WelcomeActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>+
        </activity>
        <activity android:name=".RecyclerViewActivity"></activity>
        <activity android:name="main.materialdesign.recycle.CardActivity"></activity>
        <activity android:name=".ShadowClipActivity"></activity>
        <activity android:name="main.materialdesign.toolbar.ToolbarActivity"></activity>
    </application>

</manifest>
